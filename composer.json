{
    "name": "dokobit/ws-api-sdk-php",
    "description": "Client for Dokobit.com e-signature WS API",

    "type": "library",
    "license": "MIT",
    "homepage": "https://www.dokobit.com/solutions",

    "require": {
        "php"               : "^7.0",
        "guzzlehttp/guzzle" : "^7.0",
        "symfony/validator" : "~6.0"
    },
    "require-dev": {
        "psr/log"           : "~1.0",
        "monolog/monolog"   : "~1.0",
        "phpunit/phpunit"   : "~9.5"
    },
    "autoload": {
        "psr-4": {
            "Dokobit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dokobit\\Tests\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
